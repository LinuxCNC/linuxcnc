    border: 1px solid foregnd;
    border-radius: 4;
    margin: 0 0 0 0;
    font: 32pt }

/****** RADIO BUTTON ************/
QRadioButton {
    border: 1px solid foregnd;
    border-radius: 4 }

QRadioButton::indicator {
    width: 0;
    background: backgnd }

QRadioButton:checked {
    color: backgnd;
    background: highlight }

QRadioButton:pressed {
    color: backgnd;
