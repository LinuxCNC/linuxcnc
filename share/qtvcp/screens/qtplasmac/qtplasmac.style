    border-radius: 4px;
    width: 26px;
    height: 26px }

QCheckBox::indicator:pressed {
    background: highlight }

QCheckBox::indicator:disabled {
border-color: inactive }

QCheckBox::indicator:disabled:checked {
background: inactive }

QCheckBox::indicator:checked {
    background: highlight }

QCheckBox::indicator:checked:pressed {
    background: backgnd }

