/****** DEFAULT ************/
* {
    color: foregnd;
    background: backgnd;
    font: 10pt DejaVuSans;
    outline: none }

*:disabled {
    color: inactive;
    background: backgnd;
    border-color: inactive }

/****** FRAME ************/
#gcode_frame, #dro_frame, #file_button_frame,
#machine_frame, #button_frame, #edit_frame,
#preview_frame, #file_frame, #jog_frame,
#cut_rec_frame, #thc_frame, #sensor_frame,
#arc_frame, #control_frame, #config_frame,
#config_config, #arc_config, #thc_config,
#motion_config, #probe_config, #safety_config,
#scribe_config, #spotting_config, #utility_frame,
#gui_frame, #materials_frame, #camview_frame,
#conv_frame, #stats_frame, #pmx_stats_frame,
#machinelog_frame, #ub_frame, #sd_frame {
    border: 1px solid frames;
    background: backgnd;
    border-radius: 4px }

/****** LABEL ************/
#dro_label, #file_button_label,
#machine_label, #button_label, #edit_label,
#file_label, #jog_label, #cut_rec_label,
#thc_label, #sensor_label, #arc_label, 
#control_label, #open_label, #mdi_label,
#config_conf_lbl, #arc_conf_lbl, #thc_conf_lbl,
#motion_conf_lbl, #probe_conf_lbl, 
#safety_conf_lbl, #scribe_conf_lbl,
#spotting_conf_lbl, #utility_label, #gui_label,
#camview_label, #conv_label, #stats_label,
#pmx_stats_label, #machinelog_label {
    color: backgnd;
    background: frames;
    border-top-left-radius: 2px;
    border-top-right-radius: 2px }

/****** MAX VELOCITY ************/
#max_offset_velocity_in {
    font: 11pt }

/****** TOOLTIPS ************/
QToolTip {
    font: 13px;
    padding: 4 }

/****** MATERIAL LABEL ************/
#material_label{
    color: backgnd;
    background: frames;
    border-top-left-radius: 2px }

/****** VELOCITY LABELS ************/
#velocity_label{
    color: backgnd;
    background: frames }

#velocity_show {
    color: backgnd;
    background: frames;
    border-top-right-radius: 2px }

/****** QLINEEDITS ************/
QLineEdit {
    border: 1px solid foregnd;
    border-radius: 4px;
    height: 24px }

/****** DIALOGS ************/
QFileDialog {
    min-width: 686 }

QFileDialog QListView {
    border: 1px solid foregnd;
    border-radius: 4px }

QDialog {
    border: 1px solid foregnd;
    border-radius: 4px;
    margin: 4px }

QDialog QHeaderView::section {
    background: backgnd;
    border: 1px solid foregnd;
    border-radius: 4px;
    font: italic;
    margin: 1;
    height: 20;
    padding-top: 5 }

QDialog QListView {
    font: 11pt }

QDialog QTreeView {
    border: 1px solid foregnd;
    border-radius: 4px;
    font: 11pt;
    alternate-background-color: backalt;
    padding: 2px }

QDialog QTreeView::item {
    height: 36px }

QDialog QTreeView::item:selected:active {
    color: backgnd;
    background-color: foregnd }

QDialog QTreeView::item:selected:!active {
    color: backgnd;
    background-color: foregnd }

QDialog QLabel {
    font: 11pt }

QDialog QPushButton {
    font: 11pt;
    height: 30px;
    width: 80px;
    margin: 4 }

QDialog QLineEdit {
    selection-color: foregnd;
    selection-background-color: backgnd;
    font: 11pt;
    height: 30px }

QDialog QComboBox {
    font: 11pt;
    height: 30px }

QDialog QDoubleSpinBox {
    font: 11pt }

/****** BUTTON ************/
QPushButton, QToolButton {
    border: 1px solid foregnd;
    border-radius: 4px }

QPushButton:pressed, QToolButton:pressed {
    color: backgnd;
    background: highlight }

QPushButton:checked, QToolButton:checked {
    color: backgnd;
    background: highlight }

QPushButton:checked:pressed, QToolButton:checked:pressed {
    color: foregnd;
    background: backgnd }

/****** RESET BUTTONS ************/
#feed_label, #rapid_label,
#jogs_label, #height_reset {
    border: none }

#feed_label:pressed, #rapid_label:pressed,
#jogs_label:pressed, #height_reset:pressed {
    color: foregnd;
    background: backgnd }

/****** ESTOP BUTTON ************/
#estop,
#estop:checked:pressed,
#estop:disabled {
    color: backgnd;
    background: foregnd;
    border-color: foregnd }

#estop:pressed,
#estop:checked,
#estop:disabled:checked {
    color: backgnd;
    background: e-stop;
    border-color: e-stop }

/****** ERROR LABEL ************/
#error_label {
    color: e-stop;
}

/****** TOUCHOFF BUTTON<<<<<<<<<<<<*/
#touch_x, #touch_y, #touch_z, #touch_a, #touch_b {
    font: 20pt 'DejaVuSans' }

/******    JOG BUTTON ************/
