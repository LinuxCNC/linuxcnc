<keyboard format="3.2" id="numpad" section="system" summary="Number pad for QtPlasmaC parameters and conversational">
  <include file="key_defs.xml"/>
  <box border="0.5" orientation="vertical" spacing="1.5">
    <panel filename="numpad.svg" layer="alpha">
	  <key id="TAB"  action="single-stroke" label_x_align="0.5"/>
	  <key id="AE01" action="single-stroke"/>
	  <key id="AE02" action="single-stroke"/>
	  <key id="AE03" action="single-stroke"/>
	  <key id="move" action="single-stroke"/>
	  <key id="bktb" action="single-stroke" keysym="0xfe20" label="⇤"/>
	  <key id="AE04" action="single-stroke"/>
	  <key id="AE05" action="single-stroke"/>
	  <key id="AE06" action="single-stroke"/>
	  <key id="LEFT" action="single-stroke"/>
	  <key id="AE07" action="single-stroke"/>
	  <key id="AE08" action="single-stroke"/>
	  <key id="AE09" action="single-stroke"/>
	  <key id="UP"   action="single-stroke"/>
	  <key id="RGHT" action="single-stroke"/>
	  <key id="AE11" action="single-stroke"/>
	  <key id="AE10" action="single-stroke"/>
	  <key id="AB09" action="single-stroke"/>
	  <key id="DOWN" action="single-stroke"/>
	  <key id="BKSP" action="single-stroke"/>
	  <key id="DELE" action="single-stroke"/>
	  <key id="RTRN" action="single-stroke" label_x_align="0.5"/>
    </panel>
  </box>
</keyboard>
