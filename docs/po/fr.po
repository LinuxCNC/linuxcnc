"net Xstep => parport.0.pin-03-out\n"
"net Xdir  => parport.0.pin-02-out\n"
"net Ystep => parport.0.pin-05-out\n"
"net Ydir  => parport.0.pin-04-out\n"
"net Zstep => parport.0.pin-07-out\n"
"net Zdir  => parport.0.pin-06-out\n"

