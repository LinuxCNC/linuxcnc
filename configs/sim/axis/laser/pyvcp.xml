<?xml version='1.0' encoding='UTF-8'?>
<pyvcp>
    <label>
        <text>"Laser"</text>
    </label>
    <vbox>
        <labelframe text="Output Power">
            <bar>
                <halpin>"power"</halpin>
                <min_>0</min_>
                <max_>100</max_>
                <bgcolor>"grey"</bgcolor>
                <fillcolor>"red"</fillcolor>
            </bar>
        </labelframe>
        <labelframe text="Min Power">
            <number>
                <halpin>"min-power"</halpin>
                <format>"2.2f"</format>
            </number>
        </labelframe>
        <labelframe text="Max Power">
            <number>
                <halpin>"max-power"</halpin>
                <format>"2.2f"</format>
            </number>
        </labelframe>
        <labelframe text="Vector Power">
            <number>
                <halpin>"vector-power"</halpin>
                <format>"2.2f"</format>
            </number>
        </labelframe>
        <labelframe text="Raster On">
            <led>
                <halpin>"raster-enabled"</halpin>
            </led>
        </labelframe>
        <labelframe text="Raster Program Size">
            <s32>
                <halpin>"raster-program-size"</halpin>
            </s32>
        </labelframe>
        <labelframe text="Raster Fault">
            <led>
                <halpin>"raster-fault"</halpin>
            </led>
        </labelframe>
        <button>
            <halpin>"raster-reset"</halpin>
            <text>"Reset Raster"</text>
         </button>
    </vbox>
</pyvcp>


