 N..... USE_LENGTH_UNITS(CANON_UNITS_MM)
 N..... SET_G5X_OFFSET(1, 0.0000, 0.0000, 0.0000, 0.0000, 0.0000, 0.0000)
 N..... SET_G92_OFFSET(0.0000, 0.0000, 0.0000, 0.0000, 0.0000, 0.0000)
 N..... SET_XY_ROTATION(0.0000)
 N..... SET_FEED_REFERENCE(CANON_XYZ)
 N..... COMMENT("Program to test M70 save/M72 restore context")
 N..... COMMENT("M. Haberler")
 N..... STRAIGHT_TRAVERSE(0.0000, 0.0000, 0.0000, 0.0000, 0.0000, 0.0000)
 N..... COMMENT("relative")
 N..... COMMENT("interpreter: distance mode changed to incremental")
 N..... STRAIGHT_TRAVERSE(10.0000, 10.0000, 0.0000, 0.0000, 0.0000, 0.0000)
 N..... STRAIGHT_TRAVERSE(20.0000, 20.0000, 0.0000, 0.0000, 0.0000, 0.0000)
 N..... COMMENT("save context")
 N..... COMMENT("absolute")
 N..... COMMENT("interpreter: distance mode changed to absolute")
 N..... STRAIGHT_TRAVERSE(5.0000, 20.0000, 0.0000, 0.0000, 0.0000, 0.0000)
 N..... COMMENT("restore context")
 N..... COMMENT("interpreter: distance mode changed to incremental")
 N..... COMMENT("back in relative mode due to G72")
 N..... MESSAGE(" incremental=1.000000")
 N..... STRAIGHT_TRAVERSE(0.0000, 0.0000, 0.0000, 0.0000, 0.0000, 0.0000)
 N..... COMMENT("absolute")
 N..... COMMENT("interpreter: distance mode changed to absolute")
 N..... STRAIGHT_TRAVERSE(5.0000, 20.0000, 0.0000, 0.0000, 0.0000, 0.0000)
 N..... COMMENT("relative")
 N..... COMMENT("interpreter: distance mode changed to incremental")
 N..... STRAIGHT_TRAVERSE(15.0000, 30.0000, 0.0000, 0.0000, 0.0000, 0.0000)
 N..... STRAIGHT_TRAVERSE(25.0000, 40.0000, 0.0000, 0.0000, 0.0000, 0.0000)
 N..... COMMENT("absolute")
 N..... COMMENT("interpreter: distance mode changed to absolute")
 N..... STRAIGHT_TRAVERSE(5.0000, 20.0000, 0.0000, 0.0000, 0.0000, 0.0000)
 N..... MESSAGE(" incremental=0.000000")
 N..... FINISH()
