  [\\/]* | ?:[\\/]*)
  ac_cv_path_DVIPNG="$DVIPNG" # Let the user override the test with a path.
  ;;
  *)
  as_save_IFS=$IFS; IFS=$PATH_SEPARATOR
for as_dir in $PATH
do
  IFS=$as_save_IFS
