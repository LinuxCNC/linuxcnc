  { printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: $YAPPS" >&5
printf "%s\n" "$YAPPS" >&6; }
else
  { printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: no" >&5
