	as_fn_error $? "no dvipng, HTML documentation cannot be built" "$LINENO" 5
    fi

    { printf "%s\n" "$as_me:${as_lineno-$LINENO}: checking for HTML support in groff" >&5
printf %s "checking for HTML support in groff... " >&6; }
    if ! groff -Thtml < /dev/null > /dev/null 2>&1 ; then
        as_fn_error $? "no groff -Thtml, HTML documentation cannot be built" "$LINENO" 5
    else
        { printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: yes" >&5
printf "%s\n" "yes" >&6; }
    fi

    # Extract the first word of "checklink", so it can be a program name with args.
set dummy checklink; ac_word=$2
{ printf "%s\n" "$as_me:${as_lineno-$LINENO}: checking for $ac_word" >&5
