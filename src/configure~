fi
DVIPNG=$ac_cv_path_DVIPNG
if test -n "$DVIPNG"; then
  { printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: $DVIPNG" >&5
printf "%s\n" "$DVIPNG" >&6; }
