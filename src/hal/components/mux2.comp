component mux2 "Select from one of two input values";
pin in bit sel;
pin out float out "Follows the value of in0 if sel is FALSE, or in1 if sel is TRUE";
pin in float in1;
pin in float in0;
option period no;
function _;
license "GPL";
author "Jeff Epler";
see_also "mux4(9), mux8(9), mux16(9), mux_generic(9).";
;;
FUNCTION(_) {
    if(sel) out = in1;
    else out = in0;
}
