# Check whether --enable-simulator or --disable-simulator was given.
if test "${enable_simulator+set}" = set; then
  enableval="$enable_simulator"

        case "$enableval" in
            Y*|y*)
                SIMULATOR=yes
                RTS=simulator
                BUILD_SYS=sim
