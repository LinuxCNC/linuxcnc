##############################################################################
# Subsection 2.2                                                             #
# 1. If a RT has been specified by the user it needs to be checked for       #
# validity. That is done by checking default locations for a certain script  #
# (similar to 2)                                                             #
#                                                                            #
# 2. If no RT has been specified explicitely by the user, one has to be      #
# found by search. In order to do that the following code will search for a  #
# RT-specific script:                                                        #
# rtlinux-config for RTLinux                                                 #
# realtime-config for RTAI 24.1.x                                            #
# rtai-config for RTAI 3.x                                                   #
##############################################################################

#at this point if RTDIR is empty, we need to find RT ourselves
if test $SIMULATOR = yes; then
  RTS=sim
elif test -z "$RTDIR"; then
  for location in /usr/realtime-`uname -r` /usr/realtime /usr /usr/src/rtai*; do
    for subdir in scripts bin; do
      for signature in rtai rtl realtime; do
	candidate=$location/$subdir/$signature-config
	if test -z "$RTS" -a -e $candidate; then
	  RTS=$candidate
	fi
      done
    done
 done
 if test -z "$RTS"; then
    { { echo "$as_me:$LINENO: error: RT not found.  Specify one of:
	--with-rtai=<path>
	--with-rtlinux=<path>
	--with-rtlinuxpro=<path>
or use
	--enable-simulator
to build without a realtime system." >&5
echo "$as_me: error: RT not found.  Specify one of:
	--with-rtai=<path>
	--with-rtlinux=<path>
	--with-rtlinuxpro=<path>
or use
	--enable-simulator
to build without a realtime system." >&2;}
   { (exit 1); exit 1; }; }
  fi
else
  # we have RTDIR from the user... let's check for rtai-config, realtime-config, and rtl-config
  RTDIR=$(cd $RTDIR ; pwd -P )
  if (! test "x$RTAIDIR" = "x") ; then
    # Test for rtai-config - RTAI3.x
    if (test -f $RTDIR/rtai-config); then
       RTS=$RTDIR/rtai-config
    elif (test -r $RTDIR/bin/rtai-config); then
       RTS=$RTDIR/bin/rtai-config
    elif (test `rtai-config --version 2> /dev/null`); then
       RTS=rtai-config
       #we have RTDIR defined by the user, let's check if $RTS --prefix = $RTDIR
       if (! test "$RTDIR" = `$RTS --prefix`); then
	  { echo "$as_me:$LINENO: WARNING: RTAIDIR=$RTDIR specified contains no rtai-config, however one was found in PATH" >&5
echo "$as_me: WARNING: RTAIDIR=$RTDIR specified contains no rtai-config, however one was found in PATH" >&2;}
          RTDIR=`$RTS --prefix`
	  { echo "$as_me:$LINENO: WARNING: using RTAIDIR=$RTDIR from now on" >&5
echo "$as_me: WARNING: using RTAIDIR=$RTDIR from now on" >&2;}
       fi
    # Test for realtime-config - RTAI24.1.x
    elif (test -f $RTDIR/realtime-config); then
       RTS=$RTDIR/realtime-config
    elif (test -r $RTDIR/bin/realtime-config); then
       RTS=$RTDIR/bin/realtime-config
    elif (test -r $RTDIR/scripts/realtime-config); then
       RTS=$RTDIR/scripts/realtime-config
    elif (test `realtime-config --version 2> /dev/null`); then
       RTS=realtime-config
       #we have RTDIR defined by the user, let's check if $RTS --prefix = $RTDIR
       if (! test "$RTDIR" = `$RTS --prefix`); then
	  { echo "$as_me:$LINENO: WARNING: RTAIDIR=$RTDIR specified contains no realtime-config, however one was found in PATH" >&5
echo "$as_me: WARNING: RTAIDIR=$RTDIR specified contains no realtime-config, however one was found in PATH" >&2;}
          RTDIR=`$RTS --prefix`
	  { echo "$as_me:$LINENO: WARNING: using RTAIDIR=$RTDIR from now on" >&5
echo "$as_me: WARNING: using RTAIDIR=$RTDIR from now on" >&2;}
       fi
    else
       { { echo "$as_me:$LINENO: error: RTAIDIR=$RTAIDIR specified, but neither rtai-config \(RTAI-3.x\) nor realtime-config \(rtai-24.1.x\) found. Check your RTAI install" >&5
echo "$as_me: error: RTAIDIR=$RTAIDIR specified, but neither rtai-config \(RTAI-3.x\) nor realtime-config \(rtai-24.1.x\) found. Check your RTAI install" >&2;}
   { (exit 1); exit 1; }; }
    fi

  elif (! test "x$RTLINUXDIR" = "x") ; then
    # test for rtl-config
    if (test -f $RTLINUXDIR/rtl-config); then
       RTS=$RTLINUXDIR/rtl-config
    elif (test -r $RTLINUXDIR/bin/rtl-config); then
       RTS=$RTLINUXDIR/bin/rtl-config
    elif (test -r $RTLINUXDIR/scripts/rtl-config); then
       RTS=$RTLINUXDIR/scripts/rtl-config
    fi
  elif (! test "x$RTLINUXPRODIR" = "x") ; then
    # test for RTLinuxPro
    # right now unsupported
    { { echo "$as_me:$LINENO: error: RTLinuxPro support to be added" >&5
echo "$as_me: error: RTLinuxPro support to be added" >&2;}
   { (exit 1); exit 1; }; }
  fi
  echo "$as_me:$LINENO: result: $RTDIR set from command line" >&5
echo "${ECHO_T}$RTDIR set from command line" >&6 # Spit out an error if/when it fails ??
fi

echo "$as_me:$LINENO: result: Using $RTS as the RT signature" >&5
echo "${ECHO_T}Using $RTS as the RT signature" >&6

##############################################################################
# Subsection 2.3                                                             #
# we have the RT-script now, it's time to query it for some values           #
# it can provide us with default values such as CC, CFLAGS, etc              #
# see each case for more details                                             #
##############################################################################

# For RTAI-3.x $RTS = rtai-config
if (test `echo $RTS | awk -F/ '{print $NF }'` = rtai-config); then
# Options and their return values for rtai-config
#	 Option		Returns
#	--prefix	Path to the rtai installation (usually /usr/realtime), not the original source tree.
#	--version	Version number of the installed RTAI (e.g. 3.1, 3.0r4)
#	--cc		The compiler used for the C sources - Not necessarily the default cc
#	--module-cflags	Flags used to compile the realtime modules with - It is a good idea to use these !
#	--linux-dir	Path to the source directory f the patched kernel.
#	--module-dir	Path to the directory where the rtai modules are installed
    RTNAME=rtai-3.0
    RTPREFIX=rtai
    RTAI=3
    RTDIR=`$RTS --prefix`
    RTDIR=$(cd $RTDIR ; pwd -P )
    RTFLAGS=`$RTS --module-cflags`
    RTFLAGS="$RTFLAGS -DRTAI=3"
    KERNELDIR=`$RTS --linux-dir`

# For RTAI-24.1x $RTS = realtime-config
elif (test `echo $RTS | awk -F/ '{print $NF }'` = "realtime-config"); then
# Options and their return values for realtime-config
#	 Option		Returns
#	--prefix	Path to the rtai installation (usually /usr/realtime), not the original source tree. - USE WITH CAUTION
#	--version	Should return the version number - Appears to return 0.0.0 for most 24.1.x releases.
#	--cc		The compiler used for the C sources - Not necessarily the default cc
#	--cflags	Flags used to compile the realtime modules with.
#	--cflags-fp	Flags to use when compiling realtime modules with floating point math.
#	--dump		Dumps the .buildvars to screen - Grep for linuxdir
    RTNAME=rtai
    RTPREFIX=rtai
    RTAI=1

    # on BDI-TNG there seems to be the problem,
    # that the /usr/src/linux/.config file is missing
    # this file states that the kernel has been configured,
    # however it gets deleted when a make mrproper is run
    # on BDI-TNG only the patched kernel source is included in order
    # to save space, so realtime-config is failing.
    # the following is a workaround only for BDI-TNG
    BDITNG=`cat /proc/version 2>/dev/null | grep Gariboldi`
    if test -n "$BDITNG"; then
	# we are certain to have BDI-TNG
	# now we default some values
	CC="egcs"
	RTFILE=/usr/src/rtai-24.1.10/.buildvars
	RTDIR=/usr/src/rtai-24.1.10
	KERNEL=$(cat $RTFILE | grep LINUXDIR | awk '{split($1, A, "="); print A[2]}') 2>/dev/null #'
	KERNELDIR=$(cd $KERNEL ; pwd -P )
	RTFLAGS=$(cat $RTFILE | grep CFLAGS | sed s/CFLAGS=//) 2>/dev/null #'
	RTFLAGS="$RTFLAGS -DRTAI=1"
	# the TNG disk is also missing "linux/modversions.h"
	# we need to check for it, it may have already been replaced
	echo "$as_me:$LINENO: checking for linux/modversions.h" >&5
echo $ECHO_N "checking for linux/modversions.h... $ECHO_C" >&6
	if ! test -r $KERNELDIR/include/linux/modversions.h ; then
	    # file is missing, fortunately on BDI-TNG it is a one line file, create it
            ( echo "#include <linux/modsetver.h>" >$KERNELDIR/include/linux/modversions.h ) 2>/dev/null
	    # but the user is probably not root, so that didn't work
	    if ! test -r $KERNELDIR/include/linux/modversions.h ; then
	        echo "$as_me:$LINENO: result: missing" >&5
echo "${ECHO_T}missing" >&6
	        { { echo "$as_me:$LINENO: error: modversions.h is missing.  This is a known problem on
		  the BDI-TNG distro.  Please re-run ./configure as root
		  to replace it.  You will only have to do this once." >&5
echo "$as_me: error: modversions.h is missing.  This is a known problem on
		  the BDI-TNG distro.  Please re-run ./configure as root
		  to replace it.  You will only have to do this once." >&2;}
   { (exit 1); exit 1; }; }
	    else
	        echo "$as_me:$LINENO: result: missing... replaced" >&5
echo "${ECHO_T}missing... replaced" >&6
	    fi
	else
	    echo "$as_me:$LINENO: result: ok" >&5
echo "${ECHO_T}ok" >&6
	fi
    else
	# any other rtai-24.1.x system
	TEMP=`$RTS --prefix`
	if (test -d $TEMP) ; then
	    RTDIR=$(cd $TEMP ; pwd -P )
	else
	    RTDIR=${RTS%%/scripts/realtime-config}
	fi
        RTFLAGS=`$RTS --cflags-fp | head -n 1`
	RTFLAGS="$RTFLAGS -DRTAI=1"
	KERNELDIR=$($RTS --dump | grep -i linuxdir | awk -F'[ ="]+' '{print $2}')
#"
	KERNELDIR=$(cd $KERNELDIR ; pwd -P )
    fi


# For RTLinux-3.? $RTS = rtl-config
elif (test `echo $RTS | awk -F/ '{print $NF }'` = "rtl-config"); then
# Options and their return values for rtl-config
#	 Option		Returns
#	--prefix	Path to the RTLinux installation (usually /usr/rtlinux-{rtlVersion}), not the original source tree.
#	--rtlVersion	Version number of the installed RTLinux (e.g. 3.0)
#	--cc		The compiler used for the C sources - Not necessarily the default cc
#	--cflags	Flags used to compile the realtime modules with - It is a good idea to use these !
#	--linux		Path to the source directory of the patched kernel.
#	--module_dir	Path to the directory where the RTLinux modules are installed
#	--modules	A list of the available realtime modules.
    RTPREFIX=rtl
    RTDIR=`$RTS --prefix`
    RTDIR=$(cd $RTDIR ; pwd -P )
    RTFLAGS=`$RTS --cflags`
    KERNELDIR=`$RTS --linux`
# elif test for RTLinux-Pro ???
elif (test $RTS = sim); then
    RTPREFIX=sim
    RTDIR=$(pwd -P)
    RTFLAGS=-DSIMULATOR
fi

echo "$as_me:$LINENO: checking for location of kernel headers" >&5
echo $ECHO_N "checking for location of kernel headers... $ECHO_C" >&6

# Check whether --with-kernel-headers or --without-kernel-headers was given.
if test "${with_kernel_headers+set}" = set; then
  withval="$with_kernel_headers"

        KERNELDIR="$withval"
        echo "$as_me:$LINENO: result: using specified value: $KERNELDIR" >&5
echo "${ECHO_T}using specified value: $KERNELDIR" >&6

else

        echo "$as_me:$LINENO: result: using value from $RTS: $KERNELDIR" >&5
echo "${ECHO_T}using value from $RTS: $KERNELDIR" >&6

fi;










