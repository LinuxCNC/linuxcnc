  if (! test -z "$RTDIR") ; then
    { { echo "$as_me:$LINENO: error: rtdir=$RTDIR & rtlinuxpro=$RTLINUXPRODIR defined. Only one is possible" >&5
echo "$as_me: error: rtdir=$RTDIR & rtlinuxpro=$RTLINUXPRODIR defined. Only one is possible" >&2;}
   { (exit 1); exit 1; }; }
  else
