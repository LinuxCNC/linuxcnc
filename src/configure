        TMP=$(find /lib/modules/$KERNEL_VERS -name $m$MODEXT)
    fi
    eval MODPATH_$m=${TMP/$KERNEL_VERS/\\\$\\\(uname\\\ -r\\\)}
    echo "$as_me:$LINENO: result: ${TMP:-not found}" >&5
echo "${ECHO_T}${TMP:-not found}" >&6
done




















