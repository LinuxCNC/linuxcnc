ApproxInt_MultiLine::ApproxInt_MultiLine()
{
  PtrOnmySvSurfaces = NULL;
  myLine = NULL;
  indicemin = 0;
  indicemax = 0;
  nbp3d = 0;
  nbp2d = 0;
  myApproxU1V1 = Standard_False;
  myApproxU2V2 = Standard_False;
  p2donfirst = Standard_True;
  Xo = 0.;
  Yo = 0.;
  Zo = 0.;
  U1o = 0.;
  V1o = 0.;
  U2o = 0.;
  V2o = 0.;
}

