//=======================================================================

 inline void  ChFiDS_FaceInterference::SetLastParameter(
					const Standard_Real U1)
{
  lastParam = U1;
}


//=======================================================================
//function : LineIndex
//purpose  : 
//=======================================================================

 inline Standard_Integer ChFiDS_FaceInterference::LineIndex()const 
{
