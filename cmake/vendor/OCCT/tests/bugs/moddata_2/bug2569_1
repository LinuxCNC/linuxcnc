puts ""
######################################################
# If it is not possible to create the bezier curve, 
# it should throw an exception.
######################################################
cpulimit 60
vinit
set out [OCC2569 26 result]
if {[string compare $out "\n Degree = 25\n"] == 0} {
    puts "OCC2569: OK"
} else {
    puts "OCC2569: Faulty"
}    
vfit
