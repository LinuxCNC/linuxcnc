######################################################
# Problem of Memory Leak
######################################################

#
# Result is not Null shape
#

set BugNumber OCC7287

set NCycles 10
box b1 10 10 10 100 100 100
box b2 50 50 50

set listmem {}
for {set i 1} {${i} <= ${NCycles}} {incr i } {
    bop b1 b2
    bopcommon r

    lappend listmem [meminfo h]
    checktrend $listmem 0 100 "Memory leak detected"
}
