# test script on make volume operation
# cone plane

# planar face 
plane pln_f1 62.947464405791067 -1160.0000000363409 -356.99281047643018 -0.16808376151111815 -0.25112060959207205 0.95325038083096503
erase pln_f1
mkface f1 pln_f1 -1000000 1000000 -1000000 1000000

# conical face 
cone con_f2 0 237.27272726999999 0 0 -1 1.1102230246251565e-016 14.543833993447443 0
erase con_f2
mkface f2 con_f2 0 6.2831853071795862 0 1000000

# planar face 
plane pln_f3 0 -1219.9999999945176 1.5341142011343817e-013 0 -1 1.1102230246251565e-016
erase pln_f3
mkface f3 pln_f3 -1000000 1000000 -1000000 1000000

# planar face 
plane pln_f4 0 -1280 1.6007275826179778e-013 0 1 -1.1102230246251565e-016
erase pln_f4
mkface f4 pln_f4 -1000000 1000000 -1000000 1000000

# make volume operation 
mkvolume result f1 f2 f3 f4

checkprops result -s 3.32328e+006

