# test script on make volume operation
# cylinder plane

# planar face 
plane pln_f1 0 825 4.163336342344337e-015 0 -1 -1.1102230246251565e-016
erase pln_f1
mkface f1 pln_f1 -1000000 1000000 -1000000 1000000

# planar face 
plane pln_f2 -771.03174431000002 820 -1434.59193091 -2.2204460492503131e-016 -1.1102230246251565e-016 1
erase pln_f2
mkface f2 pln_f2 -1000000 1000000 -1000000 1000000

# planar face 
plane pln_f3 -793.75341161103199 820.00000000000023 -1557.828784412869 -0.45399049973954675 1.1102230246251565e-016 -0.8910065241883679
erase pln_f3
mkface f3 pln_f3 -1000000 1000000 -1000000 1000000

# cylindrical face 
cylinder cyl_f4 0 0 -9.1038288019262836e-014 0 1 1.1102230246251565e-016 1750
erase cyl_f4
mkface f4 cyl_f4 0 6.2831853071795862 -1000000 1000000

# planar face 
plane pln_f5 -837.85723364880118 820 -1479.1857140209454 -0.89100652418836812 -2.2204460492503136e-016 0.45399049973954653
erase pln_f5
mkface f5 pln_f5 -1000000 1000000 -1000000 1000000

# planar face 
plane pln_f6 -860.57890103 820 -1513.23500145 1 1.1102230246251568e-016 2.2204460492503131e-016
erase pln_f6
mkface f6 pln_f6 -1000000 1000000 -1000000 1000000

# planar face 
plane pln_f7 0 920 1.1102230246251565e-014 0 -1 -1.1102230246251565e-016
erase pln_f7
mkface f7 pln_f7 -1000000 1000000 -1000000 1000000

# cylindrical face 
cylinder cyl_f8 0 0 9.9142916099026479e-014 0 -1 1.1102230246251565e-016 1720
erase cyl_f8
mkface f8 cyl_f8 0 6.2831853071795862 -1000000 1000000

# make volume operation 
mkvolume result f1 f2 f3 f4 f5 f6 f7 f8

checkprops result -s 2.40251e+007

