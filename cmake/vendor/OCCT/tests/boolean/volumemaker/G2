# test script on make volume operation
# cone cylinder plane

# planar face 
plane pln_f1 -306.53078964627537 -1038.2137499999999 -251.37646071372467 -0.70710678118654746 4.4408920985006262e-016 0.70710678118654768
erase pln_f1
mkface f1 pln_f1 -1000000 1000000 -1000000 1000000

# planar face 
plane pln_f2 -27.577164468182303 -1038.2137499999999 -27.577164468182389 0.70710678118654768 2.2204460492503131e-016 0.70710678118654746
erase pln_f2
mkface f2 pln_f2 -1000000 1000000 -1000000 1000000

# planar face 
plane pln_f3 0 -729 1.3849699165291441e-013 0 1 -1.1102230246251565e-016
erase pln_f3
mkface f3 pln_f3 -1000000 1000000 -1000000 1000000

# planar face 
plane pln_f4 0 -786.50743635718959 3.8387493219403887e-014 0 -1 1.1102230246251565e-016
erase pln_f4
mkface f4 pln_f4 -1000000 1000000 -1000000 1000000

# conical face 
cone con_f5 0 518.47000000000003 0 0 -1 1.1102230246251565e-016 14.999999999912038 0
erase con_f5
mkface f5 con_f5 0 6.2831853071795862 0 1000000

# planar face 
plane pln_f6 -251.37646071372464 -1038.2137499999999 -306.53078964627542 -0.70710678118654746 4.4408920985006262e-016 0.70710678118654768
erase pln_f6
mkface f6 pln_f6 -1000000 1000000 -1000000 1000000

# cylindrical face 
cylinder cyl_f7 0 0 7.5439654523279387e-014 0 1 1.1102230246251565e-016 260
erase cyl_f7
mkface f7 cyl_f7 0 6.2831853071795862 -1000000 1000000

# make volume operation 
mkvolume result f1 f2 f3 f4 f5 f6 f7

checkprops result -s 6.28616e+006

