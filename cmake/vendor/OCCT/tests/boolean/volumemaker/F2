# test script on make volume operation
# cone plane

# planar face 
plane pln_f1 0 -746.52181700999995 1844.22896887 -0.86602540378443904 -0.4736773368607301 -0.16009303715192008
erase pln_f1
mkface f1 pln_f1 -1000000 1000000 -1000000 1000000

# planar face 
plane pln_f2 0 -54.262442849244721 -204.00237530028846 0 -0.32018607430384038 0.94735467372146087
erase pln_f2
mkface f2 pln_f2 -1000000 1000000 -1000000 1000000

# conical face 
cone con_f3 0 -746.52181700999995 1844.22896887 0 0.32018607430384038 -0.94735467372146098 10.181482974865283 0
erase con_f3
mkface f3 con_f3 0 6.2831853071795862 0 1000000

# planar face 
plane pln_f4 0 85.262913376439656 -616.82482172668711 0 -0.32018607430384038 0.94735467372146098
erase pln_f4
mkface f4 pln_f4 -1000000 1000000 -1000000 1000000

# conical face 
cone con_f5 0 -213.31402524999999 266.59354373999997 0 0.32018607430384038 -0.94735467372146098 10.181482974667482 0
erase con_f5
mkface f5 con_f5 0 6.2831853071795862 0 1000000

# make volume operation 
mkvolume result f1 f2 f3 f4 f5

checkprops result -s 8.92077e+006
