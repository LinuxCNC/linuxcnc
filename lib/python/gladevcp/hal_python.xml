<glade-catalog name="gladevcp" library="gladepython" supports="libglade,gtkbuilder"
               domain="glade-3" depends="gtk+">
    <init-function>glade_python_init</init-function>
    
    <glade-widget-classes>
        <glade-widget-class name="HAL_HideTable" generic-name="hal_hidetable" title="HAL Hideable Table"/>
        <glade-widget-class name="HAL_HBox" generic-name="hal_hbox" title="HAL Sensitive HBox"/>
        <glade-widget-class name="HAL_Table" generic-name="hal_table" title="HAL Sensitive table"/>
        <glade-widget-class name="State_Sensitive_Table" generic-name="state_sensitive_table" title="State_Sensitive_table"/>
        <glade-widget-class name="HAL_ComboBox" generic-name="hal_combobox" title="HAL_ComboBox"/>
        <glade-widget-class name="HAL_Button" generic-name="hal_button" title="HAL_Button"/>
        <glade-widget-class name="HALIO_Button" generic-name="halio_button" title="HALIO_Button"/>
        <glade-widget-class name="HAL_CheckButton" generic-name="hal_checkbutton" title="HAL_CheckButton"/>
        <glade-widget-class name="HAL_SpinButton" generic-name="hal_spinbutton" title="HAL_SpinButton"/>
        <glade-widget-class name="HAL_ToggleButton" generic-name="hal_togglebutton" title="HAL_ToggleButton"/>
        <glade-widget-class name="HAL_RadioButton" generic-name="hal_radiobutton" title="HAL_RadioButton"/>
        <glade-widget-class name="HAL_HScale" generic-name="hal_hscale" title="HAL_HScale"/>
        <glade-widget-class name="HALIO_HScale" generic-name="halio_hscale" title="HALIO_HScale"/>
        <glade-widget-class name="HAL_VScale" generic-name="hal_vscale" title="HAL_VScale"/>
        <glade-widget-class name="HAL_ProgressBar" generic-name="hal_progressbar" title="HAL_ProgressBar"/>
        <glade-widget-class name="HAL_LightButton" generic-name="hal_light_button" title="HAL_LightButton">
            <properties>
            	<property id="has_hal_pins" weight="1" />
            	<property id="button_halio_pin" weight="2" />
            	<property id="create_enable_pin" weight="3" />
            	<property id="dual_color" weight="4" />
            	<property id="corner_radius" weight="6" />
                <property id="border_width" weight="7" />
                <property translatable="True" id="button_text" weight="8">
                    <visible-lines>2</visible-lines>
                </property>
                <property translatable="True" id="button_on_text" weight="9">
                	<visible-lines>2</visible-lines>
                </property>
                <property id="font_face" weight="10" />
                <property id="font_bold" weight="11" />
                <property id="font_size" weight="12" />
                <property id="font_on_color" weight="13" save-always="True"/>
                <property id="font_off_color" weight="14" save-always="True"/>
                <property id="light_on_color" weight="15" save-always="True"/>
                <property id="light_off_color" weight="16" save-always="True"/>
            	<property id="light_is_on" weight="17" />
            </properties>
        </glade-widget-class>
        <glade-widget-class name="HAL_LED" generic-name="hal_led" title="HAL LED">
            <properties>
                <property id="is_on"  weight ="1"/>
                <property id="blink_when_off"  weight ="2"/>
                <property id="has_hal_pin"  weight ="3"/>
                <property id="led_shape"  weight ="4"/>
                <property id="led_size"  weight ="5"/>
                <property id="led_blink_rate" weight ="6" save-always='True'/>
                <property id="pick_color_on"  weight ="7" optional="True"/>
                <property id="pick_color_off" weight ="8" optional="True"/>
                <property id="pick_color_blink"  weight ="9" optional="True"/>
                <property id="on_color"  weight ="10"/>
                <property id="off_color"  weight ="11"/>
                <property id="blink_color"  weight ="12"/>
            </properties>
        </glade-widget-class>
        <glade-widget-class name="HAL_Label" generic-name="hal_label" title="HAL Label"/>
        <glade-widget-class name="Status_Label" generic-name="status_label" title="Status Label"/>
        <glade-widget-class name="State_Label" generic-name="state_label" title="State Label"/>
        <glade-widget-class name="HAL_HBar" generic-name="hal_hbar" title="HAL HBar"/>
        <glade-widget-class name="HAL_VBar" generic-name="hal_vbar" title="HAL VBar"/>
        <glade-widget-class name="Override" generic-name="override" title="Override Controls">
            <properties>
                <property id="Adjustment" visible="false"/>
            </properties>
        </glade-widget-class>
        <glade-widget-class name="HAL_Gremlin" generic-name="hal_gremlin" title="HAL Gremlin"/>
        <glade-widget-class name="HAL_Meter" generic-name="hal_meter" title="HAL Meter"/>
        <glade-widget-class name="HAL_Graph" generic-name="hal_graph" title="HAL Graph"/>
        
        <glade-widget-class name="JogWheel" generic-name="jogwheel" title="Jog Wheel"/>
        <glade-widget-class name="SpeedControl" generic-name="speedcontrol" title="Speed Control">
            <properties>
                <property id="size" query="False" default="1" visible="False"/>
                <property id="spacing" query="False" default="0" visible="False"/>
                <property id="homogeneous" query="False" default="0" visible="False"/>
            </properties>
        </glade-widget-class>
        <glade-widget-class name="Hal_Dial" generic-name="hal_dial" title="Hal_Dial"/>
        
        <glade-widget-class name="PyNgcGui" generic-name="pyngcgui" title="Python NgcGui"/>
        <glade-widget-class name="HAL_GremlinPlus" generic-name="hal_gremlin_plus" title="HAL Gremlin Plus"/>
        
        <glade-widget-class name="ToolEdit" generic-name="tooledit" title="Tooleditor">
            <properties>
                <property id="size" query="False" default="1" visible="False"/>
                <property id="spacing" query="False" default="0" visible="False"/>
                <property id="homogeneous" query="False" default="0" visible="False"/>
            </properties>
        </glade-widget-class>
        <glade-widget-class name="OffsetPage" generic-name="offsetpage" title="Offsetpage">
            <properties>
                <property id="size" query="False" default="1" visible="False"/>
                <property id="spacing" query="False" default="0" visible="False"/>
                <property id="homogeneous" query="False" default="0" visible="False"/>
            </properties>
        </glade-widget-class>
        <glade-widget-class name="HAL_DRO" generic-name="hal_dro" title="HAL DRO"/>
        <glade-widget-class name="Combi_DRO" generic-name="combi_dro" title="Combi DRO">
            <properties>
                <property id="size" query="False" default="1" visible="False"/>
                <property id="spacing" query="False" default="0" visible="False"/>
                <property id="homogeneous" query="False" default="0" visible="False"/>
            </properties>
        </glade-widget-class>
        <glade-widget-class name="IconFileSelection" generic-name="icon_filechooser" title="Icon File Chooser">
            <properties>
                <property id="size" query="False" default="1" visible="False"/>
                <property id="spacing" query="False" default="0" visible="False"/>
                <property id="homogeneous" query="False" default="0" visible="False"/>
            </properties>
        </glade-widget-class>
        <glade-widget-class name="HAL_Offset" generic-name="hal_offset" title="HAL Offset"/>
        <glade-widget-class name="Calculator" generic-name="calc" title="calculator">
            <properties>
                <property id="size" query="False" default="1" visible="False"/>
                <property id="spacing" query="False" default="0" visible="False"/>
                <property id="homogeneous" query="False" default="0" visible="False"/>
            </properties>
        </glade-widget-class>
        <!-- Actions -->
        <glade-widget-class name="EMC_Action_ESTOP" generic-name="vcp_action_estop" title="VCP Action ESTOP" icon-name='gtk-stop'/>
        <glade-widget-class name="EMC_Action_ESTOP_RESET" generic-name="vcp_action_estop_reset" title="VCP Action ESTOP RESET" icon-name='gtk-stop'/>
        <glade-widget-class name="EMC_Action_ON" generic-name="vcp_action_on" title="VCP Action ON" icon-name='gtk-connect'/>
        <glade-widget-class name="EMC_Action_OFF" generic-name="vcp_action_off" title="VCP Action OFF" icon-name='gtk-disconnect'/>
        <glade-widget-class name="EMC_Action_Run" generic-name="vcp_action_run" title="VCP Action Run" icon-name='gtk-media-play-ltr'/>
        <glade-widget-class name="EMC_Action_Step" generic-name="vcp_action_run" title="VCP Action Step" icon-name='gtk-media-next-ltr'/>
        <glade-widget-class name="EMC_Action_Pause" generic-name="vcp_action_run" title="VCP Action Pause" icon-name='gtk-media-pause'/>
        <glade-widget-class name="EMC_Action_Resume" generic-name="vcp_action_run" title="VCP Action Resume" icon-name='gtk-media-pause'/>
        <glade-widget-class name="EMC_Action_Stop" generic-name="vcp_action_stop" title="VCP Action Stop" icon-name='gtk-media-stop'/>
        <glade-widget-class name="EMC_Action_MDI" generic-name="vcp_action_mdi" title="VCP Action MDI" icon-name='gtk-execute'/>
        <glade-widget-class name="EMC_Action_Home" generic-name="vcp_action_home" title="VCP Action Home" icon-name='gtk-home'/>
        <glade-widget-class name="EMC_Action_Unhome" generic-name="vcp_action_unhome" title="VCP Action Unhome" icon-name='gtk-home'/>
        
        <glade-widget-class name="EMC_ToggleAction_ESTOP" generic-name="vcp_toggleaction_estop" title="VCP ToggleAction ESTOP" icon-name='gtk-stop'/>
        <glade-widget-class name="EMC_ToggleAction_Power" generic-name="vcp_toggleaction_power" title="VCP ToggleAction Power" icon-name='gtk-disconnect'/>
        <glade-widget-class name="EMC_ToggleAction_Run" generic-name="vcp_toggleaction_run" title="VCP ToggleAction Run" icon-name='gtk-media-play-ltr'/>
        <glade-widget-class name="EMC_ToggleAction_Stop" generic-name="vcp_toggleaction_stop" title="VCP ToggleAction Stop" icon-name='gtk-media-stop'/>
        <glade-widget-class name="EMC_ToggleAction_Pause" generic-name="vcp_toggleaction_pause" title="VCP ToggleAction Pause" icon-name='gtk-media-pause'/>
        <glade-widget-class name="EMC_ToggleAction_MDI" generic-name="vcp_toggleaction_mdi" title="VCP ToggleAction MDI" icon-name='gtk-execute'/>
        
        <glade-widget-class name="EMC_RadioAction_ESTOP" generic-name="vcp_radioaction_estop" title="VCP RadioAction ESTOP" icon-name='gtk-stop'/>
        <glade-widget-class name="EMC_RadioAction_ESTOP_RESET" generic-name="vcp_radioaction_estop_reset" title="VCP RadioAction ESTOP RESET" icon-name='gtk-stop'/>
        <glade-widget-class name="EMC_RadioAction_ON" generic-name="vcp_radioaction_on" title="VCP RadioAction ON" icon-name='gtk-connect'/>
        <glade-widget-class name="EMC_RadioAction_OFF" generic-name="vcp_radioaction_off" title="VCP RadioAction OFF" icon-name='gtk-disconnect'/>
        
        <glade-widget-class name="EMC_Stat" generic-name="vcp_stat" title="VCP Stat" icon-name='gtk-properties' toplevel='yes'/>
        <glade-widget-class name="EMC_FileChooserDialog" generic-name="vcp_filechooserdialog" title="VCP FileChooserDialog"/>
        <glade-widget-class name="EMC_FileChooserButton" generic-name="vcp_filechooserbutton" title="VCP FileChooserButton"/>
        <glade-widget-class name="EMC_Action_Open" generic-name="vcp_action_open" title="VCP Action Open" icon-name='gtk-open'/>
        <glade-widget-class name="EMC_Action_Reload" generic-name="vcp_action_reload" title="VCP Action Reload" icon-name='gtk-refresh'/>
        <glade-widget-class name="EMC_Action_Save" generic-name="vcp_action_save" title="VCP Action Save" icon-name='gtk-save'/>
        <glade-widget-class name="EMC_Action_SaveAs" generic-name="vcp_action_saveas" title="VCP Action Save As" icon-name='gtk-save-as'/>
        <glade-widget-class name="EMC_SourceView" generic-name="vcp_sourceview" title="VCP SourceView"/>
        <glade-widget-class name="EMC_MDIHistory" generic-name="vcp_mdihistory" title="VCP MDIHistory">
            <properties>
                <property id="size" query="False" default="0" visible="False"/>
            </properties>
        </glade-widget-class>
        <glade-widget-class name="MacroSelect" generic-name="vcp_macro_select" title="VCP Macro Select">
            <properties>
                <property id="size" query="False" default="0" visible="False"/>
            </properties>
        </glade-widget-class>
